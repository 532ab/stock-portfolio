{
  "version": 2,
  "builds": [
    {
      "src": "backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/index.js"
    }
  ],
  "env": {
    "MONGO_URI": "@mongo_uri",
    "JWT_SECRET": "@jwt_secret",
    "FINNHUB_API_KEY": "@finnhub_api_key",
    "ALPHA_VANTAGE_API_KEY": "@alpha_vantage_api_key",
    "FRONTEND_URL": "@frontend_url"
  }
}
